<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.myapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
    <name>Weex</name>
    <description>
        A sample Weex application.
    </description>
    <author email="" href="">
        Weex Team
    </author>
    <feature name="Camera">
        <param name="category" value="module" />
        <param name="android-package" value="com.nat.weex.Camera" />
        <param name="api" value="nat/camera" />
    </feature>
    <feature name="Image">
        <param name="category" value="module" />
        <param name="android-package" value="com.nat.weex.Image" />
        <param name="api" value="nat/media/image" />
    </feature>
    <feature name="NetworkTransferModule">
        <param name="category" value="module" />
        <param name="android-package" value="com.nat.weex.NetworkTransferModule" />
        <param name="api" value="nat/network/transfer" />
    </feature>
</widget>
